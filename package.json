{
  "name": "duplicate-character-finder",
  "version": "0.0.1",
  "description": "Find duplicate characters for a given string",
  "main": "index.js",
  "engines": {
    "node": ">=12.18.1 <14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "start:dev": "nodemon",
    "build": "tsc",
    "start": "npm run build && node build/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:dev": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandip22490/duplicate-character-finder.git"
  },
  "keywords": [
    "duplicate",
    "character",
    "finder"
  ],
  "author": "Sandip Patel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sandip22490/duplicate-character-finder/issues"
  },
  "homepage": "https://github.com/sandip22490/duplicate-character-finder#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.3",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4"
  }
}
